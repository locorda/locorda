# Re-saving the exact same data with identified blank nodes (ingredients and supplier)
# This should be a no-op with no property changes
ex:it a schema:Recipe ;
    schema:name "Tomato Soup" ;
    schema:recipeIngredient [
        a schema:Ingredient ;
        schema:name "Tomato" ;
        schema:amount "2" ;
        schema:unit "cups" ;
        schema:provider [
            a schema:Organization ;
            schema:name "Local Farmers Market" ;
            schema:telephone "555-1234"
        ]
    ], [
        a schema:Ingredient ;
        schema:name "Basil" ;
        schema:amount "1" ;
        schema:unit "bunch"
        # No provider - same as stored state
    ] .
